{
  "name": "veinif",
  "version": "0.0.1-alpha.2",
  "description": "Choice-based interactive fiction framework",
  "main": "electron-src/index.js",
  "scripts": {
    "dev": "webpack serve --mode=production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc:w": "tsc -w",
